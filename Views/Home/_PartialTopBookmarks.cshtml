@model List<Tuple<Bookmark, List<string>, int>>

@foreach (var item in Model)
{
    <div>
        <h5 class="display-7">
            <a href=@item.Item1.urlLink>
                @item.Item1.name
            </a>
        </h5>
        
        <p>[@item.Item1.urlLink]</p>
        <p>Total: @item.Item3</p>
        <nobr>
            @foreach (var tag in @item.Item2)
            {
                <small>@tag </small> 
            }
        </nobr>
        <hr>
    </div>
    
}